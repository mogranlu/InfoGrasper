# some more ls aliases
alias ll='ls -l'
alias la='ls -A'
alias l='ls -CF'

#Git aliases
echo -n "alias gs='echo git status;git status'" >> ~/.bashrc
echo -n "alias ga='echo git add;git add'" >> ~/.bashrc
echo -n "alias gc='echo git commit -m;git commit -m'" >> ~/.bashrc
echo -n "alias gpull='echo git pull;git pull'" >> ~/.bashrc
echo -n "alias gpush='echo git push;git push'" >> ~/.bashrc
